<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
  <!-- Inbound methods -->
  <record id="account_payment_method_cash_in" model="account.payment.method">
    <field name="name">Cash</field>
    <field name="code">cash_in</field>
    <field name="payment_type">inbound</field>
  </record>
  <record id="account_payment_method_transfer_in" model="account.payment.method">
    <field name="name">Transfer</field>
    <field name="code">transfer_in</field>
    <field name="payment_type">inbound</field>
  </record>
  <record id="account_payment_method_cheque_in" model="account.payment.method">
    <field name="name">Cheque</field>
    <field name="code">cheque_in</field>
    <field name="payment_type">inbound</field>
  </record>
  <record id="account_payment_method_benefit_in" model="account.payment.method">
    <field name="name">Benefit</field>
    <field name="code">benefit_in</field>
    <field name="payment_type">inbound</field>
  </record>

  <!-- Outbound methods -->
  <record id="account_payment_method_cash_out" model="account.payment.method">
    <field name="name">Cash</field>
    <field name="code">cash_out</field>
    <field name="payment_type">outbound</field>
  </record>
  <record id="account_payment_method_transfer_out" model="account.payment.method">
    <field name="name">Transfer</field>
    <field name="code">transfer_out</field>
    <field name="payment_type">outbound</field>
  </record>
  <record id="account_payment_method_cheque_out" model="account.payment.method">
    <field name="name">Cheque</field>
    <field name="code">cheque_out</field>
    <field name="payment_type">outbound</field>
  </record>
  <record id="account_payment_method_benefit_out" model="account.payment.method">
    <field name="name">Benefit</field>
    <field name="code">benefit_out</field>
    <field name="payment_type">outbound</field>
  </record>
</odoo>